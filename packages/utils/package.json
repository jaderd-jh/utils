{
  "name": "@jhqn/utils",
  "type": "module",
  "version": "0.0.1-beta.9",
  "author": "jade-gjz <jiangang.zhang@jaderd.com>",
  "license": "MIT",
  "homepage": "https://github.com/jaderd-jh/utils/tree/main/packages/utils#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/jaderd-jh/utils",
    "directory": "packages/utils"
  },
  "bugs": {
    "url": "https://github.com/jaderd-jh/utils/issues"
  },
  "keywords": [
    "jade",
    "utils"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./core": {
      "types": "./dist/core.d.ts",
      "import": "./dist/core.mjs",
      "require": "./dist/core.cjs"
    },
    "./crypto": {
      "types": "./dist/crypto.d.ts",
      "import": "./dist/crypto.mjs",
      "require": "./dist/crypto.cjs"
    },
    "./faker": {
      "types": "./dist/faker.d.ts",
      "import": "./dist/faker.mjs",
      "require": "./dist/faker.cjs"
    },
    "./storage": {
      "types": "./dist/storage.d.ts",
      "import": "./dist/storage.mjs",
      "require": "./dist/storage.cjs"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.mjs",
      "require": "./dist/*.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "scripts": {
    "test": "vitest",
    "//build": "tsup",
    "build": "unbuild",
    "publint": "publint"
  },
  "dependencies": {
    "@jhqn/utils-core": "workspace:*",
    "@jhqn/utils-crypto": "workspace:*",
    "@jhqn/utils-faker": "workspace:*",
    "@jhqn/utils-storage": "workspace:*"
  }
}

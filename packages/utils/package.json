{
  "name": "@jhqn/utils",
  "version": "0.4.0",
  "keywords": [
    "jade",
    "utils"
  ],
  "homepage": "https://github.com/jaderd-jh/utils/tree/main/packages/utils#readme",
  "bugs": {
    "url": "https://github.com/jaderd-jh/utils/issues"
  },
  "license": "MIT",
  "author": "jade-gjz <jiangang.zhang@jaderd.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/jaderd-jh/utils",
    "directory": "packages/utils"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./core": {
      "types": "./dist/core.d.ts",
      "import": {
        "types": "./dist/core.d.ts",
        "default": "./dist/core.js"
      }
    },
    "./crypto": {
      "types": "./dist/crypto.d.ts",
      "import": {
        "types": "./dist/crypto.d.ts",
        "default": "./dist/crypto.js"
      }
    },
    "./faker": {
      "types": "./dist/faker.d.ts",
      "import": {
        "types": "./dist/faker.d.ts",
        "default": "./dist/faker.js"
      }
    },
    "./msw": {
      "types": "./dist/msw.d.ts",
      "import": {
        "types": "./dist/msw.d.ts",
        "default": "./dist/msw.js"
      }
    },
    "./storage": {
      "types": "./dist/storage/index.d.ts",
      "import": {
        "types": "./dist/storage/index.d.ts",
        "default": "./dist/storage/index.js"
      }
    },
    "./storage/react": {
      "types": "./dist/storage/react.d.ts",
      "import": {
        "types": "./dist/storage/react.d.ts",
        "default": "./dist/storage/react.js"
      }
    },
    "./storage/vue": {
      "types": "./dist/storage/vue.d.ts",
      "import": {
        "types": "./dist/storage/vue.d.ts",
        "default": "./dist/storage/vue.js"
      }
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": {
        "types": "./dist/*.d.ts",
        "default": "./dist/*.js"
      }
    }
  },
  "scripts": {
    "test": "vitest",
    "build": "tsdown",
    "publint": "publint"
  },
  "dependencies": {
    "@jhqn/utils-core": "workspace:*",
    "@jhqn/utils-crypto": "workspace:*",
    "@jhqn/utils-faker": "workspace:*",
    "@jhqn/utils-msw": "workspace:*",
    "@jhqn/utils-storage": "workspace:*"
  }
}

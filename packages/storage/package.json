{
  "name": "@jhqn/utils-storage",
  "type": "module",
  "version": "0.0.1-beta.27",
  "author": "jade-gjz <jiangang.zhang@jaderd.com>",
  "license": "MIT",
  "homepage": "https://github.com/jaderd-jh/utils/tree/main/packages/storage#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/jaderd-jh/utils",
    "directory": "packages/storage"
  },
  "bugs": {
    "url": "https://github.com/jaderd-jh/utils/issues"
  },
  "keywords": [
    "jade",
    "utils"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./react": {
      "types": "./dist/react.d.ts",
      "import": {
        "types": "./dist/react.d.mts",
        "default": "./dist/react.mjs"
      },
      "require": {
        "types": "./dist/react.d.cts",
        "default": "./dist/react.cjs"
      }
    },
    "./vue": {
      "types": "./dist/vue.d.ts",
      "import": {
        "types": "./dist/vue.d.mts",
        "default": "./dist/vue.mjs"
      },
      "require": {
        "types": "./dist/vue.d.cts",
        "default": "./dist/vue.cjs"
      }
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": {
        "types": "./dist/*.d.mts",
        "default": "./dist/*.mjs"
      },
      "require": {
        "types": "./dist/*.d.cts",
        "default": "./dist/*.cjs"
      }
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest",
    "build": "unbuild",
    "publint": "publint"
  },
  "peerDependencies": {
    "@vueuse/core": "^10.2.0",
    "@vueuse/shared": "^10.2.0",
    "jotai": "^2.2.1"
  },
  "peerDependenciesMeta": {
    "@vueuse/core": {
      "optional": true
    },
    "@vueuse/shared": {
      "optional": true
    },
    "jotai": {
      "optional": true
    }
  },
  "dependencies": {
    "@jhqn/utils-core": "workspace:*",
    "@jhqn/utils-crypto": "workspace:*"
  },
  "devDependencies": {
    "@vueuse/core": "^10.4.1",
    "@vueuse/shared": "^10.4.1",
    "jotai": "^2.4.3"
  }
}

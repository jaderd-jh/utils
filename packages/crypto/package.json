{
  "name": "@jhqn/utils-crypto",
  "version": "0.4.1",
  "keywords": [
    "jade",
    "utils"
  ],
  "homepage": "https://github.com/jaderd-jh/utils/tree/main/packages/crypto#readme",
  "bugs": {
    "url": "https://github.com/jaderd-jh/utils/issues"
  },
  "license": "MIT",
  "author": "jade-gjz <jiangang.zhang@jaderd.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/jaderd-jh/utils",
    "directory": "packages/crypto"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./aes": {
      "types": "./dist/aes.d.ts",
      "import": {
        "types": "./dist/aes.d.ts",
        "default": "./dist/aes.js"
      }
    },
    "./base64": {
      "types": "./dist/base64.d.ts",
      "import": {
        "types": "./dist/base64.d.ts",
        "default": "./dist/base64.js"
      }
    },
    "./base64url": {
      "types": "./dist/base64url.d.ts",
      "import": {
        "types": "./dist/base64url.d.ts",
        "default": "./dist/base64url.js"
      }
    },
    "./md5": {
      "types": "./dist/md5.d.ts",
      "import": {
        "types": "./dist/md5.d.ts",
        "default": "./dist/md5.js"
      }
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": {
        "types": "./dist/*.d.ts",
        "default": "./dist/*.js"
      }
    }
  },
  "scripts": {
    "test": "vitest",
    "build": "tsdown",
    "publint": "publint"
  },
  "dependencies": {
    "crypto-js": "catalog:"
  },
  "devDependencies": {
    "@types/crypto-js": "catalog:"
  },
  "peerDependencies": {
    "crypto-js": "^4.2.0"
  },
  "peerDependenciesMeta": {
    "crypto-js": {
      "optional": false
    }
  }
}

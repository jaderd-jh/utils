  "license": "MIT",
  "homepage": "https://github.com/jaderd-jh/utils/tree/main/packages/crypto#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/jaderd-jh/utils",
    "directory": "packages/crypto"
  },
  "bugs": {
    "url": "https://github.com/jaderd-jh/utils/issues"
  },
  "keywords": [
    "jade",
    "utils"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./aes": {
      "types": "./dist/aes.d.ts",
      "import": {
        "types": "./dist/aes.d.mts",
        "default": "./dist/aes.mjs"
      },
      "require": {
        "types": "./dist/aes.d.cts",
        "default": "./dist/aes.cjs"
      }
    },
    "./base64": {
      "types": "./dist/base64.d.ts",
      "import": {
        "types": "./dist/base64.d.mts",
        "default": "./dist/base64.mjs"
      },
      "require": {
        "types": "./dist/base64.d.cts",
        "default": "./dist/base64.cjs"
      }
    },
    "./base64url": {
      "types": "./dist/base64url.d.ts",
      "import": {
        "types": "./dist/base64url.d.mts",
        "default": "./dist/base64url.mjs"
      },
      "require": {
        "types": "./dist/base64url.d.cts",
        "default": "./dist/base64url.cjs"
      }
    },
    "./md5": {
      "types": "./dist/md5.d.ts",
      "import": {
        "types": "./dist/md5.d.mts",
        "default": "./dist/md5.mjs"
      },
      "require": {
        "types": "./dist/md5.d.cts",
        "default": "./dist/md5.cjs"
      }
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": {
        "types": "./dist/*.d.mts",
        "default": "./dist/*.mjs"
      },
      "require": {
        "types": "./dist/*.d.cts",
        "default": "./dist/*.cjs"
      }
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest",
    "build": "unbuild",
    "publint": "publint"
  },
  "peerDependencies": {

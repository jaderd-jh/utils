{
  "name": "utils",
  "type": "module",
  "version": "0.0.3-beta.4",
  "private": true,
  "packageManager": "pnpm@9.1.4",
  "description": "jade's utils monorepo",
  "author": "jade-gjz <jiangang.zhang@jaderd.com>",
  "license": "MIT",
  "keywords": ["jade", "utils"],
  "scripts": {
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "build": "pnpm -r build",
    "release": "bumpp -r",
    "check:lint": "oxlint && eslint --cache .",
    "fix:lint": "oxlint --quiet --fix && eslint --cache --fix .",
    "check:fmt": "biome check .",
    "fix:fmt": "biome check --apply .",
    "publint": "pnpm -r publint",
    "check:type": "tsc --noEmit",
    "check:staged": "lint-staged",
    "pre-commit": "run-p check:type check:staged",
    "init:hooks": "npx simple-git-hooks",
    "prepare": "run-s init:*"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.7.3",
    "@jhqn/eslint-config": "^0.7.6",
    "@types/node": "^20.14.0",
    "@vitest/coverage-v8": "^1.6.0",
    "bumpp": "^9.4.1",
    "eslint": "^9.4.0",
    "eslint-plugin-oxlint": "^0.4.0",
    "jsdom": "^24.1.0",
    "lint-staged": "^15.2.5",
    "npm-run-all2": "^6.2.0",
    "oxlint": "^0.4.2",
    "publint": "^0.2.8",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.4.5",
    "unbuild": "^2.0.0",
    "vite": "^5.2.12",
    "vitest": "^1.6.0"
  },
  "pnpm": {
    "overrides": {
      "array-includes": "npm:@nolyfill/array-includes@latest",
      "array.prototype.findlastindex": "npm:@nolyfill/array.prototype.findlastindex@latest",
      "array.prototype.flat": "npm:@nolyfill/array.prototype.flat@latest",
      "array.prototype.flatmap": "npm:@nolyfill/array.prototype.flatmap@latest",
      "es-iterator-helpers": "npm:@nolyfill/es-iterator-helpers@latest",
      "has": "npm:@nolyfill/has@latest",
      "hasown": "npm:@nolyfill/hasown@latest",
      "is-arguments": "npm:@nolyfill/is-arguments@latest",
      "is-generator-function": "npm:@nolyfill/is-generator-function@latest",
      "object.assign": "npm:@nolyfill/object.assign@latest",
      "object.entries": "npm:@nolyfill/object.entries@latest",
      "object.fromentries": "npm:@nolyfill/object.fromentries@latest",
      "object.groupby": "npm:@nolyfill/object.groupby@latest",
      "object.values": "npm:@nolyfill/object.values@latest",
      "string.prototype.padend": "npm:@nolyfill/string.prototype.padend@latest",
      "which-typed-array": "npm:@nolyfill/which-typed-array@latest"
    }
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm pre-commit"
  },
  "lint-staged": {
    "*": ["oxlint --quiet --fix", "eslint --cache --fix", "biome check --apply --no-errors-on-unmatched"]
  }
}
